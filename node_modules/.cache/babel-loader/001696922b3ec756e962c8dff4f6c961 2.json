A,GAAA,MAAA,GAAA,CAAA,CALA,CAKA;;AACA,eAAA,MAAA;AACA,OAPA,MAOA;AACA;AACA,QAAA,MAAA,GACA,CAAA,UAAA,CAAA,OAAA,KAAA,SAAA,CAAA,OAAA,EAAA,IAAA,IAAA,GAAA,EAAA,GAAA,EAAA,GAAA,EADA;AAEA,aAAA,OAAA,GAAA,MAAA,MAAA,GAAA,EAAA,GAAA,GAAA;AACA,eAAA,MAAA;AACA;AACA,KA7BA;AA8BA,IAAA,SA9BA,uBA8BA;AAAA,2BACA,KAAA,QADA;AAAA,UACA,MADA,kBACA,MADA;AAAA,UACA,MADA,kBACA,MADA;AAAA,UACA,YADA,kBACA,YADA;AAAA,UAEA,gBAFA,GAEA,IAFA,CAEA,gBAFA;AAGA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA,EAAA,MAAA,EAAA,YAAA;AACA,UAAA,MAAA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,GAAA,OAAA,EAAA;AACA,UAAA,KAAA,GAAA,IAAA,IAAA,GAAA,QAAA,EAAA;AACA,UAAA,QAAA,GAAA,IAAA,IAAA,CAAA,IAAA,EAAA,KAAA,GAAA,CAAA,EAAA,CAAA,EAAA,OAAA,EAAA;AACA,UAAA,aAAA,GAAA,IAAA,IAAA,CAAA,IAAA,EAAA,KAAA,EAAA,CAAA,EAAA,OAAA,EAAA;AAEA,UAAA,SAAA,GAAA,IAAA,GAAA,MAAA;AACA,MAAA,gBAAA,GAAA,MAAA,GAAA,QAAA;AAEA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA,EAAA,MAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA,EAAA,UAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,SAAA,EAAA,WAAA;;AAEA,UAAA,CAAA,KAAA,SAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,WAAA;AACA,aAAA,KAAA,CAAA,iBAAA,EAAA,gBAAA;AACA,QAAA,MAAA,GAAA,IAAA,CAAA,KAAA,CACA,UAAA,CAAA,gBAAA,IAAA,IAAA,GAAA,MAAA,CAAA,CADA,EAEA,cAFA,EAAA;AAGA,eAAA,MAAA;AACA,OAPA,MAOA;AACA;AACA,YAAA,aAAA,GAAA,CAAA,CAAA,GAAA,IAAA;AACA,QAAA,MAAA,GAAA,IAAA,CAAA,KAAA,CACA,UAAA,CAAA,gBAAA,GAAA,aAAA,CADA,EAEA,cAFA,EAAA;AAIA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,GAAA,aAAA,GAAA,CAAA,GAAA,IAAA,EAAA,aAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,EAAA,aAAA,EAAA,IAAA;AAEA;AACA;AACA;AACA;AACA;;AACA,eAAA,MAAA;AACA;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AACA;AAhFA;AATA,CAAA","sourcesContent":["<template>\n  <div id=\"section_2\" class=\"sections\">\n    <div class=\"text_wrap\">\n      <div class=\"text_line\" v-if=\"this.sec1Data.SalarySelect === '월급'\">\n        <h1>오늘까지 번 돈은 {{ salaryWon() }}원</h1>\n      </div>\n      <div class=\"text_line\" v-else-if=\"this.sec1Data.SalarySelect === '연봉'\">\n        <h1>\n          오늘까지 번 돈은\n          <h1 class=\"Mfont\">{{ salaryWon() }}</h1>\n          원\n        </h1>\n      </div>\n      <div class=\"text_line\">\n        <h1>\n          월급까지 남은날은\n          <small class=\"Mfont\">{{ getNextSalaryDate() }}</small\n          >일\n        </h1>\n      </div>\n      <div class=\"text_line percent_wrap\">\n        <div class=\"percent\" :style=\"{ width: percent + '%' }\">\n          <img v-if=\"percent < 50\" src=\"../image/icon2.png\" alt=\"아이콘 \" />\n          <img v-else src=\"../image/icon.png\" alt=\"아이콘 \" />\n        </div>\n        <div class=\"percent\"></div>\n      </div>\n      <div class=\"text_line\">\n        <span @click=\"moveSection('section_3')\" style=\"margin-top:35px\"\n          >이거사려먼 얼마나 벌어야하는지 알아보기! ></span\n        >\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { moveTo } from '../utils/utils';\nexport default {\n  data() {\n    return {\n      percent: '',\n      dailyIncomePrice: '',\n      minusDate: '',\n    };\n  },\n  props: ['sec1Data'],\n  methods: {\n    moveSection(goto) {\n      moveTo(goto);\n    },\n    getNextSalaryDate() {\n      let result;\n      const { payday } = this.sec1Data;\n      const year = new Date().getFullYear();\n      const date = new Date().getDate();\n      const month = new Date().getMonth();\n      const paydayDate = new Date(year, month + 1, payday);\n      const paydayDates = new Date(year, month, payday);\n\n      const todayDate = new Date(year, month, date);\n\n      if (payday - new Date().getDate() >= 0) {\n        //월급날짜가 오늘 날짜보다 숫자가 클때\n        result =\n          (paydayDates.getTime() - todayDate.getTime()) / 1000 / 60 / 60 / 24;\n        this.percent = 100 - (result / 30) * 100;\n        this.minusDate = result + 1; //현재 마이너스값인데 , 월급 다음날부터 계산에 포함해야하므로 플러스로 하루 빼준다\n        return result;\n      } else {\n        //월급날짜가 오늘 날짜보다 숫자가 작을때 (즉 월급 무조건 다음달)\n        result =\n          (paydayDate.getTime() - todayDate.getTime()) / 1000 / 60 / 60 / 24;\n        this.percent = 100 - (result / 30) * 100;\n        return result;\n      }\n    },\n    salaryWon() {\n      const { payday, salary, SalarySelect } = this.sec1Data;\n      let { dailyIncomePrice } = this;\n      console.log(payday, salary, SalarySelect);\n      let result;\n      const date = new Date().getDate();\n      const month = new Date().getMonth();\n      const lastDate = new Date(2020, month + 1, 0).getDate();\n      const lastMonthDate = new Date(2020, month, 0).getDate();\n\n      const countDate = date - payday;\n      dailyIncomePrice = salary / lastDate;\n\n      console.log(date, 'date');\n      console.log(lastDate, 'lastDate');\n      console.log(countDate, 'countDate');\n\n      if (!this.minusDate) {\n        console.log('월급날마이너스아님');\n        this.$emit('postIncomePirce', dailyIncomePrice);\n        result = Math.floor(\n          parseFloat(dailyIncomePrice * (date - payday))\n        ).toLocaleString();\n        return result;\n      } else {\n        //월급날이 마이너스 뜰때\n        const countWorkDate = -1 + date;\n        result = Math.floor(\n          parseFloat(dailyIncomePrice * countWorkDate)\n        ).toLocaleString();\n\n        console.log(payday - lastMonthDate - 1 + date, 'whatthefuck');\n        console.log(payday, lastMonthDate, date);\n\n        /* const lastDateCount = lastMonthDate - payday <= 0 ? this.minusDate : 0;\n        result = Math.floor(\n          parseFloat(dailyIncomePrice * (date + lastDateCount))\n        ).toLocaleString();\n        console.log(lastMonthDate - payday, 'whatthefuck'); */\n        return result;\n      }\n\n      /*  ---연봉계산하기--- else {\n        result = Math.floor(\n          parseFloat((salary / 365) * countDate)\n        ).toLocaleString();\n        console.log(salary / 365, '샐러리를 나눈거');\n        console.log(countDate, 'countDate');\n        return result;\n      } */\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n@import '../style/_variables.scss';\nh1 {\n  color: $mainColor;\n}\n.percent_wrap {\n  position: relative;\n  width: 60vw;\n  min-width: 400px;\n  margin-top: 30px;\n  .percent {\n    position: absolute;\n    width: 100%;\n    height: 4px;\n    border-radius: 3px;\n    background-color: $mainColor;\n  }\n  & div:first-child {\n    opacity: 1;\n    img {\n      position: absolute;\n      z-index: 9;\n      left: 100%;\n      top: -19px;\n      width: 40px;\n      opacity: 1;\n      animation: iconAnimation 1.5s infinite;\n    }\n  }\n  & div:nth-child(2) {\n    opacity: 0.4;\n  }\n}\n\n@-webkit-keyframes iconAnimation {\n  0% {\n    left: 100%;\n  }\n  70% {\n    left: 98%;\n  }\n  100% {\n    left: 100%;\n  }\n}\n</style>\n"],"sourceRoot":"src/components"}]}